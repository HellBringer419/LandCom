#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/cli.js/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/node_modules:/home/hellbringer419/Desktop/Programs/node_modules:/home/hellbringer419/Desktop/node_modules:/home/hellbringer419/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../watch/0.18.0/node_modules/watch/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/cli.js/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules/watch/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/0.18.0/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/watch/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules/.registry.npmjs.org/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/client/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/LandCom/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/LandCom/node_modules:/home/hellbringer419/Desktop/Programs/HtmlProgs/node_modules:/home/hellbringer419/Desktop/Programs/node_modules:/home/hellbringer419/Desktop/node_modules:/home/hellbringer419/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../watch/0.18.0/node_modules/watch/cli.js" "$@"
  ret=$?
fi
